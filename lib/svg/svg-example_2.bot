import os.path

# SVG is in same folder as this bot:
svg_file = os.path.dirname('__file__') + 'circles.svg'

size(600,600)

svg = ximport("svg")

# The parse() command will return
# a list of the shapes in the SVG file.
paths = svg.parse(open(svg_file).read())



for path in paths:
        # Use copies of the paths
        # that adhere to the transformations 
        # (translate, scale, rotate) we defined.




        
        drawpath(path)


